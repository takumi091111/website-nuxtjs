    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^2.6.0",
    "lint-staged": "^8.2.1",
    "prettier": "^1.16.4",
    "stylelint": "^11.0.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-order": "^3.1.1",
    "tsconfig-paths": "^3.9.0",
    "vue-svg-inline-loader": "^1.2.18"
