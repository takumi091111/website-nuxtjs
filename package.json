{
  "name": "website-nuxtjs",
  "version": "1.0.0",
  "description": "Asamac's Portfolio, and Blog",
  "author": "Asamac",
  "private": true,
  "scripts": {
    "dev": "nuxt-ts",
    "build": "nuxt-ts build",
    "start": "nuxt-ts start",
